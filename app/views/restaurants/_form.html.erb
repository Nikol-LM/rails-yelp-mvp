<%= form_for restaurant do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :category, 'Category' %>
  <%= f.text_field :category %>

  <%= f.label :address, 'Location' %>
  <%= f.text_field :address %>

  <%= f.label :phone_number, 'Tel'%>
  <%= f.text_field :phone_number %>

  <%= f.submit %>
<% end %>
